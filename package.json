{
  "name": "auto-landlord",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "scripts\\setup.bat",
    "seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "wrangler:login": "npx wrangler login",
    "wrangler:d1:create": "npx wrangler d1 create auto-landlord-db",
    "wrangler:d1:migrate": "npx wrangler d1 execute auto-landlord-db --file=./prisma/migrations/init.sql",
    "wrangler:deploy": "npx wrangler pages deploy .vercel/output/static --branch=production",
    "pages:build": "npx @cloudflare/next-on-pages",
    "promote-admin": "node scripts/promote-admin.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
